{
	"expo": {
		"name": "Remindsy",
		"description": "An app to remind you, one or two weeks before a friend's birthday or anniversary, so you have time to send a card and gift.",
		"slug": "remindsy",
		"privacy": "public",
		"sdkVersion": "42.0.0",
		"platforms": ["ios", "android"],
		"githubUrl": "https://github.com/Panda4817/remindsy",
		"orientation": "portrait",
		"primaryColor": "#381dfc",
		"version": "1.0.0",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#381dfc"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"bundleIdentifier": "dev.kmunton.remindsy",
			"buildNumber": "1.0.0",
			"supportsTablet": true
		},
		"android": {
			"package": "dev.kmunton.remindsy",
			"versionCode": 1,
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#381dfc"
			},
			"permissions": [
				"READ_CONTACTS",
				"READ_EXTERNAL_STORAGE",
				"WRITE_EXTERNAL_STORAGE",
				"READ_CALENDAR",
				"RECEIVE_BOOT_COMPLETED",
				"NOTIFICATIONS",
				"VIBRATE"
			]
		},
		"androidStatusBar": {
			"barStyle": "light-content",
			"backgroundColor": "#381dfc"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
