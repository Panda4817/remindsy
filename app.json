{
	"expo": {
		"name": "Remindsy",
		"description": "An app to remind you, one or two weeks before a friend's birthday or anniversary, so you have time to send a card and gift.",
		"slug": "remindsy",
		"privacy": "public",
		"sdkVersion": "42.0.0",
		"platforms": ["ios", "android"],
		"orientation": "portrait",
		"version": "1.0.0",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#381dfc"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#381dfc"
			}
		},
		"androidStatusBar": {
			"barStyle": "light-content",
			"backgroundColor": "#381dfc"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
