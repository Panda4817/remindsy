{
	"main": "node_modules/expo/AppEntry.js",
	"scripts": {
		"start": "expo start",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"eject": "expo eject",
		"test": "node_modules/.bin/jest",
		"test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand"
	},
	"jest": {
		"projects": [
			{
				"preset": "jest-expo/ios",
				"transformIgnorePatterns": [
					"node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@sentry/.*|@react-native-picker/picker|react-native-safe-area-context|expo-status-bar|@react-native-picker/.*)"
				]
			},
			{
				"preset": "jest-expo/android",
				"transformIgnorePatterns": [
					"node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@sentry/.*|@react-native-picker/picker|react-native-safe-area-context|expo-status-bar|@react-native-picker/.*)"
				]
			}
		],
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.{ts,tsx}",
			"!**/coverage/**",
			"!**/node_modules/**",
			"!**/babel.config.js",
			"!**/jest.setup.js"
		],
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx"
		]
	},
	"dependencies": {
		"@expo/vector-icons": "^12.0.5",
		"@react-native-community/masked-view": "^0.1.11",
		"@react-native-picker/picker": "1.9.11",
		"@react-navigation/bottom-tabs": "^5.11.10",
		"@react-navigation/material-bottom-tabs": "^5.3.15",
		"@react-navigation/native": "^5.9.4",
		"@react-navigation/stack": "^5.14.5",
		"@types/react-native-calendars": "^1.505.3",
		"@types/yup": "^0.29.11",
		"expo": "~41.0.1",
		"expo-app-loading": "^1.0.3",
		"expo-font": "^9.1.0",
		"expo-notifications": "~0.11.6",
		"expo-screen-orientation": "~3.1.0",
		"expo-sqlite": "^9.1.0",
		"expo-status-bar": "^1.0.4",
		"expo-web-browser": "^9.1.0",
		"formik": "^2.2.6",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
		"react-native-calendars": "^1.1260.0",
		"react-native-gesture-handler": "^1.10.3",
		"react-native-get-random-values": "^1.7.0",
		"react-native-paper": "^4.8.1",
		"react-native-reanimated": "^2.1.0",
		"react-native-safe-area-context": "^3.2.0",
		"react-native-screens": "^3.2.0",
		"react-native-vector-icons": "^8.1.0",
		"react-native-web": "~0.13.12",
		"react-navigation-header-buttons": "^7.0.1",
		"react-redux": "^7.2.4",
		"redux": "^4.1.0",
		"redux-thunk": "^2.3.0",
		"yup": "^0.32.9"
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@testing-library/jest-native": "^4.0.1",
		"@testing-library/react-hooks": "^5.1.2",
		"@testing-library/react-native": "^7.2.0",
		"@types/jest": "^26.0.23",
		"@types/react": "~16.9.35",
		"@types/react-native": "~0.63.2",
		"@types/react-test-renderer": "^17.0.1",
		"@types/redux-mock-store": "^1.0.2",
		"jest-expo": "^41.0.0",
		"prettier": "^2.3.0",
		"react-test-renderer": "16.13.1",
		"redux-mock-store": "^1.5.4",
		"typescript": "~4.0.0"
	},
	"private": true
}
